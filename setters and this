//Setters and "this"

class Frog
{
    String name;
    int age;
    public String getName()
    {
        return name;
    }
    public int getAge()
    {
        return age;
    }
}

class HelloWorld {
    public static void main(String[] args) {
        Frog frog1= new Frog();
        frog1.name="Toto";
        frog1.age= 1;

        System.out.println(frog1.name);
        
    }
}
